  </body>
  <script>
      function submitOptout() {
          document.getElementById("optout-form").submit();
      }
      function cancelOptout() {
          document.getElementById("prompt-modal").style.display = "none";
      }
      function promptOptout() {
          document.getElementById("prompt-modal").style.display = "block";
      }
  </script>
